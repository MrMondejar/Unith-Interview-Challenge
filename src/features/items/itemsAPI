const mock_items = [
  {
    id: 1,
    image: 'https://picsum.photos/id/4/200/300',
    title: 'title 1',
    description: 'desc1'
  },
  {
    id: 2,
    image: 'http://www.officialpsds.com/images/stocks/ALLEY-stock1502.jpg',
    title: 'title 2',
  },
  {
    id: 3,
    image: 'http://google.com',
    title: 'title 3'
  }
]
export function fetchItems(items = mock_items) {
  return new Promise<{ data: Array<unknown> }>((resolve) =>
    setTimeout(() => resolve({ data: items }), 500)
  );
}